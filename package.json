{
  "name": "guideline-aware-ai-agent",
  "version": "1.0.0",
  "description": "AI agent that dynamically incorporates behavioral guidelines from Supabase",
  "main": "dist/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon --exec ts-node src/server/index.ts",
    "dev:client": "vite",
    "build": "tsc && vite build",
    "start": "node dist/index.js",
    "setup": "ts-node scripts/setup-database.ts",
    "setup:rls": "ts-node scripts/setup-rls.ts",
    "test": "ts-node scripts/test-setup.ts"
  },
  "keywords": ["ai", "agent", "guidelines", "supabase", "typescript"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.3",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "openai": "^4.28.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.11.19",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.3",
    "vite": "^5.1.4",
    "concurrently": "^8.2.2"
  }
}
